<div class="container d-flex justify-content-center">
  <div class="row">
    <div class="d-flex justify-content-center row">
      <% @articles.each do |article| %>
        <article>
          <h2>
            <%= link_to article.title, article_path(article) %>
          </h2>
          <%= article.content %>
        </article>
      <% end %>
    </div>
  </div>
</div>
